{
  "name": "resolve-esm",
  "description": "Shim for `import.meta.resolve`",
  "version": "1.3.0",
  "author": {
    "name": "nujarum",
    "url": "https://github.com/nujarum"
  },
  "bugs": {
    "url": "https://github.com/nujarum/resolve-esm/issues"
  },
  "dependencies": {
    "callsites": "~4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "concurrently": "^7.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.1",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-ts": "^3.0.2",
    "ts-jest": "^29.0.1",
    "typescript": "^4.8.3"
  },
  "engines": {
    "node": ">=14"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "types/**/*.d.ts"
  ],
  "homepage": "https://github.com/nujarum/resolve-esm#readme",
  "imports": {
    "#*": "./dist/*.js"
  },
  "keywords": [
    "ecmascript",
    "esm",
    "import",
    "import.meta.resolve",
    "meta",
    "mjs",
    "module",
    "ponyfill",
    "resolve",
    "shim",
    "url"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": "github:nujarum/resolve-esm",
  "scripts": {
    "build": "rollup -c",
    "clean": "rimraf .cache dist types",
    "jest": "cross-env \"NODE_OPTIONS='--experimental-vm-modules --no-warnings'\" jest",
    "lint": "eslint . --cache --cache-file .cache/.eslintcache",
    "prepare": "husky install",
    "test": "concurrently npm:jest npm:lint"
  },
  "type": "module",
  "types": "./types/index.d.ts"
}
