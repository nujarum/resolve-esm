{
  "name": "resolve-esm",
  "description": "Shim for `import.meta.resolve`",
  "version": "1.4.2",
  "author": {
    "name": "nujarum",
    "url": "https://github.com/nujarum"
  },
  "bugs": {
    "url": "https://github.com/nujarum/resolve-esm/issues"
  },
  "dependencies": {
    "callsites": "~4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@types/jest": "^29.5.3",
    "@types/node": "^18.17.3",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.19.0",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.46.0",
    "eslint-plugin-unicorn": "^46.0.1",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "rimraf": "^5.0.1",
    "rollup": "^3.28.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "ts-jest-resolver": "^2.0.1",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=14"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "types/**/*.d.ts"
  ],
  "homepage": "https://github.com/nujarum/resolve-esm#readme",
  "imports": {
    "#*": {
      "types": "./types/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "keywords": [
    "ecmascript",
    "esm",
    "import",
    "import.meta.resolve",
    "meta",
    "mjs",
    "module",
    "ponyfill",
    "resolve",
    "shim",
    "url"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": "github:nujarum/resolve-esm",
  "scripts": {
    "build": "concurrently -c auto npm:tsc npm:rollup",
    "clean": "rimraf .cache dist types",
    "jest": "cross-env \"NODE_OPTIONS='--experimental-vm-modules --no-warnings'\" jest",
    "lint": "eslint . --cache --cache-file .cache/.eslintcache",
    "prepare": "husky install",
    "rollup": "rollup -c",
    "test": "concurrently -c auto npm:lint npm:jest",
    "tsc": "tsc -b src"
  },
  "type": "module",
  "types": "./types/index.d.ts",
  "volta": {
    "node": "18.17.1"
  }
}
